  "dist": "PUT YOUR DIST PATH HIRE! IT MUST BE A PATH TO THEME SUBDIRECTORY IN WORDPRESS THEMES DIRECTORY!",
  "sass": {
    "main_files_glob": "src/sass/*",
    "main_files_dist": "css",
    "style_css_src": "src/sass/main.sass"
  },
  "general_locals":{
    "shiba":"inu"
  },
  "jade": {
    "main_globs": [
      "src/*.jade"
    ],
    "sub_globs": [
      {
        "from": "src/inc/*.jade",
        "to": "inc"
      }
    ],
    "locals":{
      "test":"test"
    }
  },
  "copy": {
    "from_src": [
      {
        "from": "js/app.js",
        "to": "js/app.js"
      },
      {
        "from": "functions.php",
        "to": "functions.php"
      }
    ],
    "from_res": [
      {
        "from": "img",
        "to": "img"
      }
    ],
    "from_bow": {
      "jquery": [
        {
          "from": "dist/jquery.js",
          "to": "js/jquery.js"
        },
        {
          "from": "dist/jquery.min.js",
          "to": "js/jquery.min.js"
        }
      ],
      "bootstrap": [
        {
          "from": "dist/js/bootstrap.js",
          "to": "js/bootstrap.js"
        },
        {
          "from": "dist/js/bootstrap.min.js",
          "to": "js/bootstrap.min.js"
        }
      ]
    },
